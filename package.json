{
  "name": "mashup-univers",
  "author": "Antoine Renault <antoine.renault.mmi@gmail.com>",
  "version": "0.1.0",
  "description": "Mashup univers",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --colors",
    "build": "npm run clean && NODE_ENV=production webpack",
    "lint": "eslint src config",
    "lint:fix": "npm run lint -- --fix",
    "deploy:now": "npm run build && cd dist && now --static -p -n mashup-univers  && cd ..",
    "deploy:gae:staging": "npm run build && gcloud app deploy --version=staging --no-promote && gcloud app browse && gcloud app logs tail -s default",
    "deploy:gae:production": "npm run build && gcloud app deploy --version production && gcloud app browse && gcloud app logs tail -s default"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "preload-webpack-plugin": "^1.2.2",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "dotenv-webpack": "^1.5.4",
    "hoek": "^5.0.4",
    "jquery": "^3.3.1",
    "mime": "^1.6.0",
    "normalize.css": "^7.0.0",
    "typed.js": "^2.0.4",
    "uglify-js": "^3.4.7"
  }
}
